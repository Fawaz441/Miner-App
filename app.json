{
  "expo": {
    "name": "miner",
    "slug": "miner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff00"
      },
      "package": "com.fawaz441.miner"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "description": "",
    "plugins": [
      "expo-task-manager",
      "expo-background-fetch"
    ]
  }
}
